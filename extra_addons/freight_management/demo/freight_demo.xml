        <field name="code">OIL_PKG</field>
        <field name="name">Oil Equipment Package</field>
        <field name="is_container">False</field>
        <field name="container_type">package</field>
        <field name="max_weight">8000</field>
        <field name="length">4.0</field>
        <field name="width">3.0</field>
        <field name="height">3.0</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">False</field>
        <field name="description">Specialized packaging for oil & gas equipment</field>
    </record>

    <record id="package_geophysical" model="freight.container">
        <field name="code">GEO_PKG</field>
        <field name="name">Geophysical Equipment Package</field>
        <field name="is_container">False</field>
        <field name="container_type">package</field>
        <field name="max_weight">3000</field>
        <field name="length">2.5</field>
        <field name="width">2.0</field>
        <field name="height">2.0</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">True</field>
        <field name="description">Precision packaging for sensitive geophysical instruments</field>
    </record>

</data>
</odoo>
