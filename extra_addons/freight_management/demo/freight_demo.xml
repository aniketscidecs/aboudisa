        <field name="code">KWKWI</field>
        <field name="name">Port of Kuwait</field>
        <field name="country_id" ref="base.kw"/>
        <field name="ocean_supported">True</field>
        <field name="land_supported">True</field>
        <field name="air_supported">False</field>
    </record>

    <record id="port_doha_air" model="freight.port">
        <field name="code">DOH</field>
