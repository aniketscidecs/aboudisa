    <!-- Additional Airlines for Regional Coverage -->
    <record id="airline_qatar" model="freight.airline">
        <field name="code">QR</field>
        <field name="name">Qatar Airways Cargo</field>
        <field name="country_id" ref="base.qa"/>
        <field name="iata_code">QR</field>
        <field name="icao_code">QTR</field>
        <field name="airline_type">cargo</field>
    </record>

    <record id="airline_etihad" model="freight.airline">
        <field name="code">EY</field>
        <field name="name">Etihad Cargo</field>
        <field name="country_id" ref="base.ae"/>
        <field name="iata_code">EY</field>
        <field name="icao_code">ETD</field>
        <field name="airline_type">cargo</field>
    </record>

    <!-- Additional Vessels for Different Routes -->
    <record id="vessel_msc_demo" model="freight.vessel">
        <field name="code">MSC002</field>
        <field name="name">MSC Gulsun</field>
        <field name="country_id" ref="base.ch"/>
        <field name="vessel_type">container</field>
        <field name="global_zone">Europe - Middle East</field>
