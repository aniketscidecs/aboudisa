    <record id="vessel_hapag_demo" model="freight.vessel">
        <field name="code">HAPAG003</field>
        <field name="name">Hapag-Lloyd Berlin Express</field>
        <field name="country_id" ref="base.de"/>
        <field name="vessel_type">container</field>
        <field name="global_zone">Asia - Middle East</field>
    </record>

    <!-- Additional Containers for Different Industries -->
    <record id="container_open_top" model="freight.container">
        <field name="code">40OT</field>
        <field name="name">40ft Open Top Container</field>
        <field name="is_container">True</field>
        <field name="container_type">open_top</field>
        <field name="size">40</field>
        <field name="max_weight">45000</field>
        <field name="length">12.19</field>
        <field name="width">2.44</field>
        <field name="height">2.59</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">False</field>
        <field name="description">Perfect for over-height cargo in oil & gas industry</field>
    </record>

    <record id="container_reefer" model="freight.container">
        <field name="code">40RF</field>
        <field name="name">40ft Refrigerated Container</field>
        <field name="is_container">True</field>
