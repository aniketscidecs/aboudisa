<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">

    <!-- Default Incoterms 2020 -->
    <record id="incoterm_exw" model="freight.incoterm">
        <field name="code">EXW</field>
        <field name="name">Ex Works</field>
        <field name="incoterm_group">e</field>
        <field name="transport_mode">any</field>
        <field name="risk_transfer_point">Seller's premises</field>
        <field name="cost_responsibility">Buyer pays all transport costs</field>
        <field name="year_version">2020</field>
    </record>

    <record id="incoterm_fob" model="freight.incoterm">
        <field name="code">FOB</field>
        <field name="name">Free on Board</field>
        <field name="incoterm_group">f</field>
        <field name="transport_mode">sea_inland</field>
        <field name="risk_transfer_point">On board vessel at port of shipment</field>
        <field name="cost_responsibility">Seller pays costs to load goods on board</field>
        <field name="year_version">2020</field>
    </record>

    <record id="incoterm_cfr" model="freight.incoterm">
        <field name="code">CFR</field>
        <field name="name">Cost and Freight</field>
        <field name="incoterm_group">c</field>
        <field name="transport_mode">sea_inland</field>
        <field name="risk_transfer_point">On board vessel at port of shipment</field>
        <field name="cost_responsibility">Seller pays freight to destination port</field>
        <field name="year_version">2020</field>
    </record>

    <record id="incoterm_cif" model="freight.incoterm">
        <field name="code">CIF</field>
        <field name="name">Cost, Insurance and Freight</field>
        <field name="incoterm_group">c</field>
        <field name="transport_mode">sea_inland</field>
        <field name="risk_transfer_point">On board vessel at port of shipment</field>
        <field name="cost_responsibility">Seller pays freight and insurance to destination port</field>
        <field name="year_version">2020</field>
    </record>

    <record id="incoterm_dap" model="freight.incoterm">
        <field name="code">DAP</field>
        <field name="name">Delivered at Place</field>
        <field name="incoterm_group">d</field>
        <field name="transport_mode">any</field>
        <field name="risk_transfer_point">At named place of destination</field>
        <field name="cost_responsibility">Seller pays all costs to named place</field>
        <field name="year_version">2020</field>
    </record>

    <record id="incoterm_ddp" model="freight.incoterm">
        <field name="code">DDP</field>
        <field name="name">Delivered Duty Paid</field>
        <field name="incoterm_group">d</field>
        <field name="transport_mode">any</field>
        <field name="risk_transfer_point">At named place of destination</field>
        <field name="cost_responsibility">Seller pays all costs including duties</field>
        <field name="year_version">2020</field>
    </record>

    <!-- Standard Container Types -->
    <record id="container_20dc" model="freight.container">
        <field name="code">20DC</field>
        <field name="name">20ft Dry Container</field>
        <field name="is_container">True</field>
        <field name="container_type">dry</field>
        <field name="size">20</field>
        <field name="volume">33.2</field>
        <field name="max_weight">28230</field>
        <field name="length">5.9</field>
        <field name="width">2.35</field>
        <field name="height">2.39</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">False</field>
    </record>

    <record id="container_40dc" model="freight.container">
        <field name="code">40DC</field>
        <field name="name">40ft Dry Container</field>
        <field name="is_container">True</field>
        <field name="container_type">dry</field>
        <field name="size">40</field>
        <field name="volume">67.7</field>
        <field name="max_weight">30480</field>
        <field name="length">12.03</field>
        <field name="width">2.35</field>
        <field name="height">2.39</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">False</field>
    </record>

    <record id="container_40hc" model="freight.container">
        <field name="code">40HC</field>
        <field name="name">40ft High Cube Container</field>
        <field name="is_container">True</field>
        <field name="container_type">dry</field>
        <field name="size">40</field>
        <field name="volume">76.4</field>
        <field name="max_weight">30480</field>
        <field name="length">12.03</field>
        <field name="width">2.35</field>
        <field name="height">2.70</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">False</field>
    </record>

    <record id="container_20rf" model="freight.container">
        <field name="code">20RF</field>
        <field name="name">20ft Refrigerated Container</field>
        <field name="is_container">True</field>
        <field name="container_type">reefer</field>
        <field name="refrigerated">True</field>
        <field name="size">20</field>
        <field name="volume">28.3</field>
        <field name="max_weight">27700</field>
        <field name="length">5.44</field>
        <field name="width">2.29</field>
        <field name="height">2.27</field>
        <field name="ocean_compatible">True</field>
        <field name="land_compatible">True</field>
        <field name="air_compatible">False</field>
    </record>

</data>
</odoo>
