<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">

    <!-- Shipment Sequence -->
    <record id="seq_freight_shipment" model="ir.sequence">
        <field name="name">Freight Shipment</field>
        <field name="code">freight.shipment</field>
        <field name="prefix">SH%(year)s-</field>
        <field name="suffix"></field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="implementation">standard</field>
    </record>

    <!-- Quotation Sequence -->
    <record id="seq_freight_quotation" model="ir.sequence">
        <field name="name">Freight Quotation</field>
        <field name="code">freight.quotation</field>
        <field name="prefix">QT%(year)s-</field>
        <field name="suffix"></field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="implementation">standard</field>
    </record>

</data>
</odoo>
